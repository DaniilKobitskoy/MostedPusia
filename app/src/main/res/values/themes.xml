<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Most" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="android:windowFullscreen">true</item>
        <item name="colorPrimary">@color/version_number_5_purple_500</item>
        <item name="colorPrimaryVariant">@color/version_number_5_purple_700</item>
        <item name="colorOnPrimary">@color/version_number_5_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/version_number_5_teal_200</item>
        <item name="colorSecondaryVariant">@color/version_number_5_teal_700</item>
        <item name="colorOnSecondary">@color/version_number_5_black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>